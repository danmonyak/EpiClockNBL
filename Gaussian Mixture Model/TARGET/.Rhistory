right_join(IMC01 %>% select(PATIENT, Q28), by = "PATIENT") %>%
mutate(treatment = ifelse(Q28 == 1,"Lofexidine", "Placebo")) %>%
select(-Q28)
# 2.sysBP
sysBP_data <- IMC13 %>%
select(PATIENT,STDYDAY, Q12SYS)
# 3. Join
new_MHOWS <- new_MHOWS %>%
inner_join(sysBP_data, by = c("PATIENT", "STDYDAY"))
new_MHOWS <- new_MHOWS %>%
